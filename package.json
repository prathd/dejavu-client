  "name": "dejavu-client",
  "next-unused": {
    "alias": {
      "@app/*": ".",
      "@lib/*": "./lib",
      "@components/*": "./components",
      "@graphql/*": "graphql"
    },
    "debug": true,
    "entrypoints": [],
    "exclude": [
      "node_modules"
    ],
    "include": [
      "components",
      "lib",
      "layouts",
      "graphql",
      "public",
      "shared",
      "styles",
      "types"
    ]
  },
  "sass": {
    "includePaths": [
      "src",
      "node_modules"
    ]
  },
  "scripts": {
    "build": "yarn prod:build",
    "db:drop": "echo 'Nothing to do!'",
    "db:reset": "echo 'Nothing to do!'",
    "db:seed": "echo 'Nothing to do!'",
    "dev": "yarn dev:start",
    "dev:reset": "yarn reinstall",
    "dev:reset:start": "yarn dev:reset && yarn dev:start",
    "dev:setup": "yarn",
    "dev:start": "next",
    "dev:start:all": "yarn dev:start",
    "docker:down": "echo 'Nothing to do!'",
    "docker:up": "echo 'Nothing to do!'",
    "find:unused": "next-unused",
    "format": "prettier '{components,graphql,layouts,lib,pages,shared,static,styles}/**/*.{js,ts,tsx,json,md}' --write && yarn lint --fix",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pre-commit": "yarn lint-staged --quiet",
    "prod": "yarn prod:start",
    "prod:build": "next build",
    "prod:reset": "yarn reinstall && yarn build:prod",
    "prod:reset:start": "yarn prod:reset && yarn prod:start",
    "prod:start": "export HEROKU_SLUG_COMMIT=$(git rev-parse HEAD) && next start -p $PORT",
    "reinstall": "rimraf .next && rimraf node_modules && yarn",
    "start": "yarn prod",
    "stylelint": "stylelint **/*",
    "test": "echo 'Nothing to test!",
    "typecheck": "tsc --noEmit"
  },
  "version": "1.0.0"
}
